    "eslint-plugin-security-node": "1.0.12",
    "husky": "4.2.5",
    "jest": "26.1.0",
    "prettier": "2.0.5",
    "react": "16.13.1",
    "react-docgen-typescript-loader": "3.7.2",
    "react-dom": "16.13.1",
    "snyk": "1.349.0",
    "source-map-loader": "1.0.0",
    "ts-jest": "^26.1.1",
    "ts-loader": "7.0.5",
    "typescript": "3.9.5",
    "webpack": "4.43.0",
    "webpack-cli": "3.3.12",
    "webpack-dev-server": "3.11.0"
