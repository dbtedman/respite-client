  "license": "MIT",
  "private": true,
  "scripts": {
    "_prettier": "prettier './**/*.{md,yml,yaml,json,ts,js}'",
    "_eslint": "yarn eslint . --ext .js,.ts",
    "start": "webpack-dev-server --mode development --open --hot",
